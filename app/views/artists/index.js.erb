<% if collection.blank? %>
    $("[data-container='artists']").html("<%= j(render('blank').strip.html_safe) %>")
<% else %>
    $("[data-container='artists']").html("<%= j(render(partial: 'artists/artist', collection: collection).strip.html_safe) %>");
    $("[data-container='pagination']").html("<%= j(render(partial: 'artists/pagination_info').strip.html_safe) %>");    
<% end %>