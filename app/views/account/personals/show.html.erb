<div id="personals">
    
    <%= render "sidebar" %>

  <div class="content">
    <header>Personals</header>
    <%= simple_form_for resource, url: account_personal_path do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :avatar, label: "Profile picture" %>
      <div class="input">
        <%= f.button :submit, value: "Update", class: "btn" %>
      </div>
    <% end %>
  </div>
</div>