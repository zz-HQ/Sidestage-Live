<%= simple_form_for :review, url: account_profile_reviews_path(parent), remote: true, class: "review-form" do |f| %>
    <%= star_rating editable: true, f: f, label: true %>
    <%= f.input :body, as: :text, placeholder: "Your review", label: false %>
    <%= f.submit class: "btn", value: "Send" %>
<%- end -%>