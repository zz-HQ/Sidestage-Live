<%= content_tag :div, data: {review: picture.picture.url(:preview)}, id: dom_id(picture), class: "thumb" do %>
	<%= image_tag picture.picture.url(:thumb) %>
  <%= link_to "<i class='fa fa-trash-o'></i>".html_safe, account_profile_picture_path(picture.imageable, picture), method: :delete, data: {remote: true, confirm: "#{t(:"helpers.confirm.sure")}"}, class: "delete" %>
<% end %>