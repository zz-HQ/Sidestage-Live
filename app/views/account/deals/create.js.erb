vex.closeAll();
<% if resource.errors.present? %>
    vex.closeAll();
    vex.open({ content: "<%= j render("form").strip.html_safe %>" });
<% else %>
    vex.dialog.alert("<h2>Your booking request was sent to <%= resource.artist.profile_name %> on <%= resource.start_at %></h2><p>We'll notify you once your request is accepted or not.</p> <br/> <br/> <p>If you need to cancel your booking request, you can do so by going to Message > <%= resource.artist.profile_name %>.</p> <%= link_to "Cancel booking request", cancel_account_deal_path(resource), method: :put, class: "cancel-link" %>").html();
<% end %>