<head>
	<title>Sidestage</title>

	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<meta name="title" content="Sidestage" />
	<meta name="description" content="Sidestage" />
	<meta name="keywords" content="" />

	<meta name="author" content="" />
	<meta name="copyright" content="" />
	<meta name="application-name" content="Sidestage" />

	<!-- for Facebook -->
	<meta property="og:title" content="Sidestage" />
	<meta property="og:type" content="website" />
	<meta property="og:image" content="http://sidestage.herokuapp.com" />
	<meta property="og:url" content="<%= request.url %>" />
	<meta property="og:description" content="Sidestage" />

	<!-- for Twitter -->
	<meta name="twitter:card" content="summary" />
	<meta name="twitter:title" content="Sidestage" />
	<meta name="twitter:description" content="Sidestage" />
	<meta name="twitter:image" content="http://sidestage.herokuapp.com/assets/logo.png" />

	<link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
	<link rel="icon" href="favicon.ico" type="image/x-icon">


	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
	
	<%= yield :html_head %>
	
</head>
